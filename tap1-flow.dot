digraph {
    node[shape=box, style=rounded]

    {rank=same; Accepted, Draft, "Final/Active"}

    Draft -> Accepted 
    Accepted -> "Final/Active"
    Draft -> Rejected
    Draft -> Withdrawn
    Draft -> Deferred
    Deferred -> Draft
    "Final/Active" -> Superseded
}